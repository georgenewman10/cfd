FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sample;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs            ("libsampling.so");

interpolationScheme cell;

setFormat       raw;

sets
(
    simple_line 
    {
        type    lineCell;
        axis    xyz;
        start   (0.353 0.353 0);
        end     (4 4 0);
    }
);

sets
(
    simple_line 
    {
        type    lineCell;
        axis    xyz;
        start   (0 0.5 0);
        end     (0 4 0);
    }
);

sets
(
    simple_line 
    {
        type    lineCell;
        axis    xyz;
        start   (-0.353 0.353 0);
        end     (-2.5 2.5 0);
    }
);

fields          ( p U );


